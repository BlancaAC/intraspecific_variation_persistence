pol.assem <- degree.id %>% dplyr::select(-c(degree, species.specificity.index, Plant_id))
#%>%  group_by(Plot) %>%
#  dplyr::summarise(across(starts_with("Acmaeodera_sp"):ends_with("Xylocopa_cantabrita"),
#                          sum, .names = "sum_{col}"))
glimpse(pol.assem)
species_cols <- pol.assem %>% dplyr::select(-Plot) %>% names()
# Compute Bray-Curtis dissimilarity matrix
dist_matrix <- vegdist(pol.assem[, species_cols], method = "bray")
# Run PERMANOVA to check for significant differences between plots
permanova_results <- adonis2(dist_matrix ~ Plot, data = pol.assem)
permanova_results
